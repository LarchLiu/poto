{
  "name": "block-plugins-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/*.ts --format cjs,esm --dts --splitting --clean",
    "dev": "tsup src/*.ts --format cjs,esm --dts --watch src"
  },
  "devDependencies": {
    "@poto/types": "workspace:*",
    "tsup": "^6.2.2"
  }
}
